<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!-->
<html lang="fr">
<!--<![endif]-->

<!-- Head BEGIN -->
<head>
  <meta charset="utf-8">
  <title>{% block title %}{% endblock %}</title>

  <meta content="width=device-width, initial-scale=1.0" name="viewport">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

  <meta content="Pièces détachées, vente, entretien, restrauration et renovation de voiture de collection de marques anglaises : Triumph, MG, Jaguar, Austin-Healey, Mini, Rover et Lotus." name="description">
  <meta content="voiture de collection, voiture anglaise, pieces detachees, restauration, renovation, Triumph, MG, Jaguar, Austin-Healey, Mini, Rover, Lotus" name="keywords">
  <meta content="Atelier Des Portes" name="author">

  <meta property="og:site_name" content="www.atelier-des-portes.fr">
  <meta property="og:title" content="{% block og_title %}{% endblock %}">
  <meta property="og:description" content="Pièces détachées, vente, entretien, restrauration et renovation de voiture de collection de marques anglaises : Triumph, MG, Jaguar, Austin-Healey, Mini, Rover et Lotus.">
  <meta property="og:type" content="website">
  <meta property="og:image" content="-CUSTOMER VALUE-">
  <meta property="og:url" content="http://www.atelier-des-portes.fr">

  <link rel="shortcut icon" href="favicon.ico">

  <!-- Fonts START -->
  <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700|PT+Sans+Narrow|Source+Sans+Pro:200,300,400,600,700,900&amp;subset=all" rel="stylesheet" type="text/css">
  <link href="http://fonts.googleapis.com/css?family=Source+Sans+Pro:200,300,400,600,700,900&amp;subset=all" rel="stylesheet" type="text/css">
  <!-- Fonts END -->

  <!-- Global styles START -->          
  <link href="{{ asset('bundles/atelierdesportes/global/plugins/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet">
  <link href="{{ asset('bundles/atelierdesportes/global/plugins/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet">
  <!-- Global styles END --> 
   
  <!-- Page level plugin styles START -->
  {% block page_style %}{% endblock %}
  <!-- Page level plugin styles END -->

  <!-- Theme styles START -->
  <link href="{{ asset('bundles/atelierdesportes/global/css/components.css') }}" rel="stylesheet">
  <link href="{{ asset('bundles/atelierdesportes/layout/css/style.css') }}" rel="stylesheet">
  <link href="{{ asset('bundles/atelierdesportes/pages/css/style-shop.css') }}" rel="stylesheet" type="text/css">
  <link href="{{ asset('bundles/atelierdesportes/pages/css/style-layer-slider.css') }}" rel="stylesheet">
  <link href="{{ asset('bundles/atelierdesportes/layout/css/style-responsive.css') }}" rel="stylesheet">
  <link href="{{ asset('bundles/atelierdesportes/layout/css/themes/red.css') }}" rel="stylesheet" id="style-color">
  <link href="{{ asset('bundles/atelierdesportes/layout/css/custom.css') }}" rel="stylesheet">
  <!-- Theme styles END -->
</head>
<!-- Head END -->

<!-- Body BEGIN -->
<body class="ecommerce">

    {{ include('SiteBundle:Header:header.html.twig') }}

    <div class="main">
      <div class="container">
        {% block content %}{% endblock %}
      </div>
    </div>

    {{ include('SiteBundle:Footer:footer.html.twig') }}

    <!-- Load javascripts at bottom, this will reduce page load time -->
    <!-- BEGIN CORE PLUGINS (REQUIRED FOR ALL PAGES) -->
    <!--[if lt IE 9]>
    <script src="{{ asset('bundles/atelierdesportes/global/plugins/respond.min.js') }}"></script>  
    <![endif]-->
    <script src="{{ asset('bundles/atelierdesportes/global/plugins/jquery-1.11.0.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/atelierdesportes/global/plugins/jquery-migrate-1.2.1.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/atelierdesportes/global/plugins/bootstrap/js/bootstrap.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/atelierdesportes/layout/scripts/back-to-top.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/atelierdesportes/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js') }}" type="text/javascript"></script>
    <!-- END CORE PLUGINS -->

    <!-- BEGIN PAGE LEVEL JAVASCRIPTS (REQUIRED ONLY FOR CURRENT PAGE) -->
    {% block page_script %}{% endblock %}
    <!-- END PAGE LEVEL JAVASCRIPTS -->
    
</body>
<!-- END BODY -->
</html>